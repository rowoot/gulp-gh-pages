{
	"name": "gulp-gh-pages",
	"version": "0.6.0-0",
	"description": "gulp plugin to publish contents to Github pages",
	"repository": "shinnn/gulp-gh-pages",
	"license": "MIT",
	"author": "Micheal Benedict <micheal@visionmasterdesigns.com> (https://github.com/rowoot)",
	"contributors": [
		"Shinnosuke Watanabe (https://github.com/shinnn)"
	],
	"keywords": [
		"git",
		"push",
		"commit",
		"branch",
		"deploy",
		"deployment",
		"publish",
		"site",
		"website",
		"gulp",
		"gulpplugin",
		"gh-pages",
		"dist",
		"github"
	],
	"files": [
		"index.js",
		"lib"
	],
	"scripts": {
		"pretest": "eslint index.js test.js lib",
		"test": "nyc --reporter=html --reporter=text mocha test.js --timeout 50000 --full-trace"
	},
	"dependencies": {
		"fancy-log": "^1.3.2",
		"gift": "^0.10.2",
		"plugin-error": "^1.0.1",
		"rimraf": "^2.6.2",
		"vinyl-fs": "^3.0.3",
		"wrap-promise": "^1.0.1"
	},
	"devDependencies": {
		"@shinnn/eslint-config-node": "^6.0.0",
		"eslint": "^5.1.0",
		"log-symbols": "^2.2.0",
		"mocha": "^5.2.0",
		"nyc": "^12.0.2",
		"octonode": "^0.9.2",
		"read-remove-file": "^5.0.1",
		"uuid": "^3.2.1",
		"vinyl": "^2.2.0"
	},
	"eslintConfig": {
		"extends": "@shinnn/node",
		"env": {
			"mocha": true
		},
		"rules": {
			"camelcase": "off",
			"no-underscore-dangle": "off",
			"promise/prefer-await-to-then": "off"
		}
	}
}
